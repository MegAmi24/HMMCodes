Code "Enable Super Peel Out" in "Gameplay/Sonic 3 & Knuckles" by "MegAmi" does "Enables Sonic's Super Peel Out move from Sonic CD."
//
    #lib "RSDK"
//
{
    if (RSDK.GetRSDKGlobalsPtr() == 0)
        return;

    *(int*)(RSDK.GetRSDKGlobalsPtr() + 0xC2CB4) |= (int)RSDK.S3KMedalMods.SuperPeelOut;
}
